{
  "name": "@sadais/mcp-pixabay-server",
  "version": "1.0.2",
  "description": "MCP server for interacting with Pixabay API based on Node",
  "license": "MIT",
  "author": "Pixabay MCP Server",
  "type": "module",
  "main": "build/index.js",
  "bin": {
    "mcp-pixabay-server": "build/index.js"
  },
  "files": [
    "build",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755') && require('fs').chmodSync('build/sse-server.js', '755')\"",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sadais-org/mcp-pixabay-server"
  },
  "keywords": [
    "mcp",
    "pixabay",
    "api",
    "image-search",
    "video-search",
    "ai",
    "claude",
    "model-context-protocol"
  ],
  "bugs": {
    "url": "https://github.com/yourusername/mcp-pixabay-server/issues"
  },
  "homepage": "https://github.com/yourusername/mcp-pixabay-server#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "vitest": "3.0.9",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/yargs": "^17.0.33",
    "typescript": "^5.3.3"
  }
}
