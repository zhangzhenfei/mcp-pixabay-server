{
  "name": "@sadais/mcp-pixabay-server",
  "version": "1.0.2",
  "description": "MCP Pixabay API server",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": {
    "mcp-pixabay-server": "build/server.js"
  },
  "scripts": {
    "start": "node build/server.js",
    "build": "rimraf build && tsc && chmod +x build/server.js",
    "watch": "tsc -w",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "inspector": "mcp-inspector debug -- node build/server.js"
  },
  "keywords": [
    "mcp",
    "pixabay",
    "ai",
    "api"
  ],
  "author": "sadais",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^2.6.12"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.8.2",
    "@types/node": "^20.10.5",
    "@types/node-fetch": "^2.6.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "files": [
    "build/**/*"
  ],
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sadais-org/mcp-pixabay-server.git"
  },
  "bugs": {
    "url": "https://github.com/sadais-org/mcp-pixabay-server/issues"
  },
  "homepage": "https://github.com/sadais-org/mcp-pixabay-server#readme"
}
